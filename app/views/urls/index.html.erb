
  <h3>My URLs</h3>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Short URL ID</th>
        <th scope="col">Long URL</th>
      </tr>
    </thead>
    <tbody>
      <% @urlsDatabase.each do |id, value| %>
        <tr>
          <td><%= id %></td>
          <td><%= value %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
